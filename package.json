{
  "name": "package-size-inline",
  "displayName": "Package Size Inline",
  "description": "Shows package sizes (weight) inline next to dependencies in package.json",
  "version": "0.1.3",
  "publisher": "El1k-1",
  "repository": {
    "type": "git",
    "url": "https://github.com/El1k-1/package-size-inline.git"
  },
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "package",
    "size",
    "npm",
    "dependencies",
    "bundle",
    "weight",
    "inline"
  ],
  "license": "MIT",
  "homepage": "https://github.com/El1k-1/package-size-inline#readme",
  "bugs": {
    "url": "https://github.com/El1k-1/package-size-inline/issues"
  },
  "activationEvents": [
    "onLanguage:json"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Package Size Inline",
      "properties": {
        "packageSizeInline.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable inline package size display"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "buildvsix": "vsce package"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "^18.x",
    "typescript": "^5.0.0"
  }
}
